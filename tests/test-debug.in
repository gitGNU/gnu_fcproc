# Test to ensure that the test environment has a good set-up
# Report from Francesco Salvestrini

required="diff"

. $testdir/defs || exit 1

cat > ./rules <<EOF
EOF

rm -f test1.log
rm -f test2.log

$FCP_NOFLAGS         --dump-rules -r ./rules > test1.log || exit 1
$FCP_NOFLAGS --debug --dump-rules -r ./rules > test2.log || exit 1

diff test1.log test2.log || {
     rm -f test1.log
     rm -f test2.log
     exit 0
}

exit 1
