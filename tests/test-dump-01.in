. $testdir/defs || exit 1

required="diff"

cat > ./rules <<EOF
ext5:ext6
	command

ext4:ext5
	command4

ext3:ext4
	command3

ext2:ext3
	command2

ext1:ext2
	command1
EOF

$FCP_NOFLAGS --dump-rules -r ./rules > test1.log || exit 1

cat > test2.log <<EOF
ext1 -> ext2
ext2 -> ext3
ext3 -> ext4
ext4 -> ext5
ext5 -> ext6
EOF

diff test1.log test2.log || exit 1

exit 0
